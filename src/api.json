{
    "swagger": "2.0",
    "info": {
        "title": "UiTdatabank v3",
        "description": "Version 3 of the UiTdatabank, a central database of cultural offers in the Flanders region.\\n Most operations require that you authenticate first with your UiTID.",
        "contact": {
            "name": "CultuurNet Vlaanderen vzw",
            "url": "http://www.cultuurnet.be",
            "email": "info@uitdatabank.be"
        },
        "version": "3.0.0"
    },
    "host": "io.uitdatabank.be",
    "basePath": "/",
    "schemes": [
        "http"
    ],
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "tags": [
        {
            "name": "User",
            "description": "Operations related to users"
        },
        {
            "name": "Place",
            "description": "Operations related to places."
        },
        {
            "name": "Event",
            "description": "Operations related to events."
        },
        {
            "name": "Offer",
            "description": "Operations related to all offer types."
        },
        {
            "name": "Label",
            "description": "Operations related to labels."
        }
    ],
    "paths": {
        "/user": {
            "$ref": "user/user.path.json"
        },
        "/events/": {
            "$ref": "event/event.path.json"
        },
        "/events/{cdbid}": {
            "$ref": "event/event-detail.path.json"
        },
        "/events/{cdbid}/audience": {
            "$ref": "event/event-audience.path.json"
        },
        "/events/{cdbid}/bookingInfo": {
            "$ref": "event/event-booking-info.path.json"
        },
        "/events/{cdbid}/contactPoint": {
            "$ref": "event/event-contact-point.path.json"
        },
        "/events/{cdbid}/labels/": {
            "$ref": "event/event-add-label.path.json"
        },
        "/events/{cdbid}/labels/{label}": {
            "$ref": "event/event-delete-label.path.json"
        }
    }
}